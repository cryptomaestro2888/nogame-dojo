mod Utils {
    #[derive(Copy, Drop, Serde)]
    struct Cost {
        steel: u128,
        quartz: u128,
        tritium: u128,
    }

    fn pow(mut base: u128, mut power: u128) -> u128 {
        // Return invalid input error
        if base == 0 {
            panic_with_felt252('II')
        }

        let mut result = 1;
        loop {
            if power == 0 {
                break result;
            }

            if power % 2 != 0 {
                result = (result * base);
            }
            base = (base * base);
            power = power / 2;
        }
    }
}

